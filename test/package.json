{
  "name": "test",
  "version": "1.0.0",
  "description": "code-server tests",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "test": "./scripts/test.sh",
    "test:jest": "jest --verbose",
    "test:docker": "cd .. && docker run --rm -v \"${PWD}:/code-server\" code-server-e2e bash -c 'source ~/.bashrc; cd /code-server/test/tmp && yarn test:jest'",
    "build:docker": "cd .. && docker build -t code-server-e2e -f code-server-e2e.dockerfile ."
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/jest-environment-puppeteer": "^4.0.0",
    "@types/node-fetch": "^2.3.2",
    "@types/puppeteer": "^1.12.3",
    "@types/xml2js": "^0.4.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/../scripts/test-setup.js"
    ],
    "moduleNameMapper": {
      "@coder/logger": "<rootDir>/node_modules/@coder/logger/out/main.js"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/logger/"
    ],
    "testRegex": ".*\\.test\\.tsx?"
  },
  "dependencies": {
    "@coder/logger": "^1.1.3",
    "@google-cloud/storage": "^2.5.0",
    "jest": "^24.7.1",
    "node-fetch": "^2.3.0"
  }
}